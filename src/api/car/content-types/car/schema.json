{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "variant": {
      "type": "string",
      "default": "1.5T CVT Ultimate",
      "required": true
    },
    "engine": {
      "type": "string",
      "default": "1.5L",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "image_slider": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "colour": {
      "type": "enumeration",
      "enum": [
        "Silver Premium Double Colour",
        "Green Premium Double Colour",
        "Black",
        "Dark Blue",
        "Gray Premium",
        "Vanilla Blue",
        "White Premium Double Colour"
      ],
      "required": true
    },
    "mileage": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "body_type": {
      "type": "string",
      "required": true,
      "default": "SUV"
    },
    "fuelType": {
      "type": "enumeration",
      "enum": [
        "Petrol",
        "Diesel",
        "Hybrid"
      ],
      "default": "Petrol",
      "required": true
    },
    "door": {
      "type": "string",
      "default": "5",
      "required": true
    },
    "year": {
      "default": 2023,
      "required": true,
      "type": "integer"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "Manual",
        "Automatic"
      ],
      "default": "Automatic",
      "required": true
    },
    "exterior_colour": {
      "type": "string",
      "required": true
    },
    "seats": {
      "type": "string",
      "default": "5",
      "required": true
    },
    "body_type_category": {
      "type": "enumeration",
      "enum": [
        "SUV"
      ],
      "default": "SUV"
    },
    "model_name": {
      "type": "enumeration",
      "enum": [
        "C5"
      ],
      "default": "C5",
      "required": true
    },
    "make_name": {
      "type": "enumeration",
      "enum": [
        "Omoda"
      ],
      "default": "Omoda",
      "required": true
    },
    "stock_number": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
